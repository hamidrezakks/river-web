{
  "name": "river",
  "version": "0.25.0",
  "private": true,
  "description": "River chat",
  "author": "Hamidreza KK <hamidrezakks@gmail.com>",
  "license": "Ronak",
  "homepage": "",
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "PUBLIC_URL=/ GENERATE_SOURCEMAP=true NODE_OPTIONS=--max_old_space_size=4096 react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "elec2": "electron --inspect=5858 .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -mwl",
    "elec-dist": "npx build --x64 --macos --win --linux --c.mac.identity=null --c.extraMetadata.main=public/electron.js -p always",
    "elec-dist-mac": "electron-builder -m",
    "elec-dist-win": "electron-builder -w",
    "elec-dist-linux": "electron-builder -l"
  },
  "dependencies": {
