{
  "name": "river",
  "version": "0.3.0",
  "private": true,
  "description": "River chat",
  "author": "Hamidreza KK <hamidrezakks@gmail.com>",
  "license": "Ronak",
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all start-js watch-css",
    "build-js": "react-scripts-ts build",
    "build": "npm-run-all build-css build-js",
    "postinstall": "mkdir -p node_modules/@material-ui/core/node_modules/@types/jss && cp -r node_modules/@types/jss node_modules/@material-ui/core/node_modules/@types",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "elec": "concurrently \"cross-env BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron --inspect=5858 .\"",
    "elec2": "electron --inspect=5858 .",
    "pack": "electron-builder --dir",
    "elec-dist": "npx build --x64 --macos --win --linux --c.mac.identity=null --c.extraMetadata.main=public/electron.js -p always",
    "elec-dist-mac": "npx build --x64 --macos --c.mac.identity=null --c.extraMetadata.main=public/electron.js -p always",
    "elec-dist-win": "npx build --x64 --win --c.mac.identity=null --c.extraMetadata.main=public/electron.js -p always",
    "elec-dist-linux": "npx build --x64 --linux --c.mac.identity=null --c.extraMetadata.main=public/electron.js -p always"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "@types/react-avatar-editor": "^10.3.2",
    "@types/react-custom-scrollbars": "^4.0.5",
    "@types/xregexp": "^3.0.29",
    "axios": "^0.18.0",
    "browser-image-resizer": "^1.2.0",
    "dexie": "^2.0.4",
    "electron-context-menu": "^0.11.0",
    "electron-debug": "^2.0.0",
    "electron-dl": "^1.12.0",
    "electron-is-dev": "^1.0.1",
    "emoji-mart": "^2.9.2",
    "faker": "^4.1.0",
    "file-saver": "^2.0.0",
    "fine-uploader": "^5.16.2",
    "fine-uploader-wrappers": "^1.0.1",
    "firebase": "^5.5.8",
    "glur": "^1.1.2",
    "google-protobuf": "^3.7.0-rc.2",
    "lodash": "^4.17.11",
    "material-ui-chip-input": "1.0.0-beta.9",
    "moment": "^2.24.0",
    "opus-recorder": "^6.1.0",
    "protobufjs": "^6.8.8",
    "rc-progress": "^2.2.6",
    "react": "^16.8.1",
    "react-avatar-editor": "^11.0.6",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.8.1",
    "react-dropzone": "^5.0.1",
    "react-fine-uploader": "^1.1.1",
    "react-image-crop": "^6.0.12",
    "react-mentions": "^2.4.1",
    "react-phone-input-2": "^2.6.0",
    "react-router-dom": "^4.3.1",
    "react-sortable-hoc": "^0.8.3",
    "react-textarea-autosize": "^7.0.4",
    "react-virtualized": "^9.21.0",
    "xregexp": "^4.2.0"
  },
  "devDependencies": {
    "@types/emoji-mart": "^2.8.2",
    "@types/faker": "^4.1.3",
    "@types/file-saver": "^2.0.0",
    "@types/google-protobuf": "^3.2.7",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.5.6",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-dropzone": "^4.2.2",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-textarea-autosize": "^4.3.3",
    "@types/react-virtualized": "^9.18.12",
    "electron": "^4.0.4",
    "electron-builder": "^20.38.5",
    "electron-devtools-installer": "^2.2.4",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "react-async-script-loader": "^0.3.0",
    "react-scripts-ts": "^3.1.0",
    "ts-protoc-gen": "^0.9.0",
    "typescript": "^3.3.3"
  },
  "build": {
    "appId": "com.your-domain",
    "compression": "maximum",
    "productName": "River",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "mac": {
      "icon": "assets/icon.png",
      "target": [
        "dmg"
      ],
      "category": "messaging"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.png"
    },
    "linux": {
      "icon": "assets/icon.png",
      "target": [
        "snap",
        "AppImage"
      ],
      "description": "Example",
      "category": "Network;Messaging"
    }
  }
}
