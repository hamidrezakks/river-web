    "glur": "^1.1.2",
    "google-protobuf": "^3.11.4",
    "int64-buffer": "^0.99.1007",
    "js-crc": "^0.2.0",
    "js-sha256": "^0.9.0",
    "jsqr": "^1.2.0",
    "kk-modality": "^1.0.5",
    "libphonenumber-js": "^1.7.22",
    "lodash": "^4.17.15",
    "markdown-it": "^10.0.0",
    "material-ui-chip-input": "^1.1.0",
    "md5-webworker": "^0.0.5",
    "moment": "^2.24.0",
    "moment-jalaali": "^0.8.3",
    "music-metadata-browser": "^1.2.1",
    "notistack": "^0.9.9",
