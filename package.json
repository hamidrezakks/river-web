    "faker": "^4.1.0",
    "fine-uploader": "^5.16.2",
    "fine-uploader-wrappers": "^1.0.1",
    "firebase": "^5.5.8",
    "google-protobuf": "^3.6.1",
    "lodash": "^4.17.10",
    "material-ui-chip-input": "^1.0.0-beta.8",
    "moment": "^2.23.0",
    "protobufjs": "^6.8.8",
    "rc-progress": "^2.2.6",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-dropzone": "^5.0.1",
