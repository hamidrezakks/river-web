{
  "name": "river",
  "version": "0.9.5",
  "private": true,
  "description": "River chat",
  "author": "Hamidreza KK <hamidrezakks@gmail.com>",
  "license": "Ronak",
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all start-js watch-css",
    "build-js": "PUBLIC_URL=/ GENERATE_SOURCEMAP=true NODE_OPTIONS=--max_old_space_size=4096 react-scripts-ts build",
    "build": "npm-run-all build-css build-js",
    "postinstall": "mkdir -p node_modules/@material-ui/core/node_modules/@types/jss && cp -r node_modules/@types/jss node_modules/@material-ui/core/node_modules/@types",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "elec": "concurrently \"cross-env BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron --inspect=5858 .\"",
    "elec2": "electron --inspect=5858 .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -mwl",
    "elec-dist": "npx build --x64 --macos --win --linux --c.mac.identity=null --c.extraMetadata.main=public/electron.js -p always",
    "elec-dist-mac": "npx build --x64 --macos --c.mac.identity=null --c.extraMetadata.main=public/electron.js -p always",
    "elec-dist-win": "electron-builder -w",
    "elec-dist-linux": "npx build --x64 --linux --c.mac.identity=null --c.extraMetadata.main=public/electron.js -p always"
  },
  "dependencies": {
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/lab": "3.0.0-alpha.30",
    "@material-ui/styles": "^4.1.2",
    "@sentry/browser": "^5.1.1",
    "@types/jss": "^9.5.8",
    "add": "^2.0.6",
    "animated-scroll-to": "1.3.1",
    "axios": "^0.19.0",
    "browser-image-resizer": "^1.2.0",
    "dexie": "^2.0.4",
    "dom-css": "^2.1.0",
    "electron-context-menu": "^0.13.0",
    "electron-debug": "^2.0.0",
    "electron-dl": "^1.14.0",
    "electron-is-dev": "^1.1.0",
    "emoji-mart": "^2.11.1",
    "file-saver": "^2.0.2",
    "fine-uploader": "^5.16.2",
    "fine-uploader-wrappers": "^1.0.1",
    "firebase": "^6.4.0",
    "glur": "^1.1.2",
    "google-protobuf": "^3.10.0",
    "int64-buffer": "^0.99.1007",
    "js-crc": "^0.2.0",
    "js-sha256": "^0.9.0",
    "jsqr": "^1.2.0",
    "libphonenumber-js": "^1.7.22",
    "lodash": "^4.17.15",
    "material-ui-chip-input": "1.0.0-beta.17",
    "md5-webworker": "^0.0.5",
    "moment": "^2.24.0",
    "moment-jalaali": "^0.8.3",
    "music-metadata-browser": "^1.2.1",
    "notistack": "^0.9.4",
    "opus-recorder": "^6.1.1",
    "protobufjs": "^6.8.8",
    "rc-progress": "^2.4.1",
    "react": "^16.10.2",
    "react-avatar-editor": "^11.0.6",
    "react-custom-scrollbars": "ronaksoft/react-custom-scrollbars",
    "react-dom": "^16.10.2",
    "react-dropzone": "^5.0.1",
    "react-fine-uploader": "^1.1.1",
    "react-google-maps": "^9.4.5",
    "react-image-crop": "^8.0.2",
    "react-intl-tel-input": "^7.0.1",
    "react-mentions": "^3.1.1",
    "react-router-dom": "^4.3.1",
    "react-virtualized": "^9.21.1",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "resize-observer-polyfill": "^1.5.1",
    "shortid": "^2.2.15",
    "stringz": "^2.0.0",
    "vcards-js": "^2.10.0",
    "xregexp": "^4.2.0",
    "yarn": "^1.17.3"
  },
  "devDependencies": {
    "@types/emoji-mart": "^2.8.4",
    "@types/faker": "^4.1.3",
    "@types/file-saver": "^2.0.0",
    "@types/google-protobuf": "^3.2.7",
    "@types/googlemaps": "^3.30.19",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/markerclustererplus": "^2.1.33",
    "@types/moment-jalaali": "^0.7.4",
    "@types/node": "^10.5.6",
    "@types/react": "^16.9.5",
    "@types/react-avatar-editor": "^10.3.2",
    "@types/react-dom": "^16.9.1",
    "@types/react-dropzone": "^4.2.2",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-textarea-autosize": "^4.3.3",
    "@types/react-virtualized": "^9.21.2",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.1",
    "@types/shortid": "^0.0.29",
    "@types/xregexp": "^3.0.29",
    "electron": "^6.0.11",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "grpc": "^1.12.4",
    "node-gyp": "^4.0.0",
    "node-pre-gyp": "^0.13.0",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "react-async-script-loader": "^0.3.0",
    "react-scripts-ts": "^3.1.0",
    "ts-protoc-gen": "0.9.0",
    "typescript": "^3.6.3"
  },
  "build": {
    "appId": "com.river.im",
    "compression": "maximum",
    "productName": "River",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "mac": {
      "icon": "assets/icon.png",
      "target": [
        "dmg"
      ],
      "category": "messaging"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.png"
    },
    "linux": {
      "icon": "assets/icon.png",
      "target": [
        "deb"
      ],
      "description": "River chat",
      "category": "Network;Messaging"
    },
    "nsis": {
      "deleteAppDataOnUninstall": "true"
    }
  }
}
